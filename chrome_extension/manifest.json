{
    "manifest_version": 2,

    "name": "四川大学网教课程统计插件",
    "description": "四川大学网教课程统计插件",
    "version": "1.0",
    "author": "hteen",

    "homepage_url": "https://github.com/hteen/scude_chrome_extension",

    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "cookies",
        "alarms",
        "activeTab",
        "background",
        "storage",
        "http://*.scude.cc/*",
        "http://scude.hteen.cn/*"
    ],

    "background": {
        "scripts": ["js/jquery-3.2.1.min.js", "js/background.js"],
        "persistent": false
    }

}
